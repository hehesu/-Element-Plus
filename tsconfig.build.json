{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "allowJs": false,
    "skipLibCheck": true
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.vue"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "**/*.vue.js",
    "**/*.js",
    "**/*.test.ts",
    "**/*.spec.ts"
  ]
}

